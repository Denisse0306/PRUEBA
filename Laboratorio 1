
#importamos libreria pandas
import pandas as pd

#Asignamos valores a la matriz data
data = [[1,"antonio",20],[2,"jose",34],[3,"luis",24],[4,"mario",25]]
#creamos dataframe con la matriz data
df = pd.DataFrame(data, columns=["id", "name", "age"])
#imprimimos dataframe dfS
print(df)

